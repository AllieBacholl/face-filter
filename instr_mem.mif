WIDTH         = 32;      -- 32 bits per entry
DEPTH         = 10;      -- 10 instructions total
ADDRESS_RADIX = UNS;     -- decimal addresses
DATA_RADIX    = HEX;     -- hex-formatted instruction words

CONTENT BEGIN
  0 : 00100093;  -- addi x1, x0, 1
  1 : 00000000;  -- NOP (or unused)
  2 : 00000000;
  3 : 00000000;
  4 : 00000000;
  5 : 00000000;
  6 : 00000000;
  7 : 00000000;
  8 : 00000000;
  9 : 00000000;
END;
